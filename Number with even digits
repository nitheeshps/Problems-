class Solution {
public:
    int findNumbers(vector<int>& nums) {
        vector<string>s;
        
        int i,count=0;
        for(i=0;i<nums.size();i++)
        {
            s.push_back(to_string(nums[i]));
        if(s[i].length()%2==0)
            {
                count++;
            }
            
        }
        return count;
        
    }
};
